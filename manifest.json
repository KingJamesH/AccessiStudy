{
    "manifest_version": 3,
    "name": "Extension",
    "version": "1.0",
    "description": "Launchacks 2025 [CHANGE]",
    "permissions": [
      "activeTab",
      "storage",
      "scripting"
    ],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [{
      "resources": ["contentScript.js", "styles.css"],
      "matches": ["<all_urls>"]
    }],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "css": ["styles.css"],
        "run_at": "document_end"
      }
    ]
  }
  